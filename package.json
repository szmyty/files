{
    "$schema": "https://json.schemastore.org/package.json",
    "name": "react-components",
    "version": "1.0.0",
    "author": {
        "name": "Alan Szmyt",
        "email": "szmyty@gmail.com"
    },
    "type": "module",
    "license": "MIT",
    "private": true,
    "scripts": {
        "tailwind-config-viewer": "npx tailwind-config-viewer --config ./tailwind.config.js serve --port 5030 --open",
        "sync": "pnpm install && pnpm prune"
    },
    "engineStrict": true,
    "packageManager": "pnpm@9.14.1",
    "pnpm": {
        "requiredScripts": [
            "build"
        ]
    },
    "engines": {
        "node": ">=20.17.0",
        "pnpm": ">=9.14.1",
        "npm": ">=10.8.2"
    },
    "devDependencies": {
        "@commitlint/cli": "19.5.0",
