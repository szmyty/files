        "react-helmet-async": "2.0.5",
        "react-router-dom": "6.28.0",
        "react-virtuoso": "4.12.0"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest",
            "google"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.stories.*"
                ],
                "parserOptions": {
                    "project": "./tsconfig.eslint.json"
                },
                "rules": {
                    "import/no-anonymous-default-export": "off"
                }
            }
        ]
    },
    "dependencies": {
